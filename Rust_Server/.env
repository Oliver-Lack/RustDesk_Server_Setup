# RustDesk Server Environment Configuration
# You can modify these settings as needed

# RustDesk Version
RUSTDESK_VERSION=1.2.3

# Server Configuration
RUSTDESK_RELAY_SERVER=localhost:21117

# Network Configuration
RUSTDESK_SIGNAL_PORT=21115
RUSTDESK_SIGNAL_TCP_UDP_PORT=21116
RUSTDESK_RELAY_PORT=21117
RUSTDESK_RELAY_PORT_2=21118
RUSTDESK_RELAY_PORT_3=21119

# Docker Configuration
COMPOSE_PROJECT_NAME=rustdesk

# Logging
LOG_LEVEL=info

# Data Persistence
DATA_DIR=./data
LOGS_DIR=./logs
